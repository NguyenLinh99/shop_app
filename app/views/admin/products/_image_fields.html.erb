<div class="nested-fields row">
  <div class="col-sm-7 input-file">
    <%= f.file_field :image_url, multiple: true %>
    <%= f.hidden_field :image_url_cache, value: f.object.image_url_cache %>
  </div>
  <div class="thumb col-sm-3">
    <% if f.object.image_url.url.present? %>
      <%= image_tag f.object.image_url.url %>
    <% end %>
  </div>
  <div class="col-sm-2 remove">
     <%= link_to_remove_association '<i class="fa fa-times"></i>'.html_safe, f, class: "remove-pic" %>
  </div>

</div>
